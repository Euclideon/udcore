<?xml version="1.0" encoding="utf-8"?>
<AutoVisualizer xmlns="http://schemas.microsoft.com/vstudio/debugger/natvis/2010">
  <Type Name="udChunkedArray&lt;*,*&gt;">
    <Expand>
      <Item Name="[size]">length</Item>
      <Item Name="[capacity]">chunkCount * $T2</Item>
